{
  "name": "jscg",
  "version": "0.0.1",
  "description": "Short challenges, puzzles, and comparisons written in Typescript/Javascript for when I have free time",
  "main": "",
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist; rm -r .nyc_output",
    "lint": "eslint **/*.ts",
    "lint-fix": "eslint **/*.ts --fix",
    "test": "mocha -r ts-node/register test/test*.ts",
    "coverage": "nyc --reporter=text npm run test",
    "all": "npm run lint && npm run coverage && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longhorn396/code-golf.git"
  },
  "author": "Devin Drawhorn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/longhorn396/code-golf/issues"
  },
  "homepage": "https://github.com/longhorn396/code-golf#readme",
  "dependencies": {
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/readline-sync": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
